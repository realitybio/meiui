{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "25cec5ff-152d-4f78-a960-f2439f639c24"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "chart": {
      "projectType": "library",
      "root": "projects/chart",
      "sourceRoot": "projects/chart/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/chart/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/chart/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/chart/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/chart/src/test.ts",
            "tsConfig": "projects/chart/tsconfig.spec.json",
            "karmaConfig": "projects/chart/karma.conf.js"
          }
        }
      }
    },
    "administration": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/administration",
      "sourceRoot": "projects/administration/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/administration",
            "index": "projects/administration/src/index.html",
            "main": "projects/administration/src/main.ts",
            "polyfills": "projects/administration/src/polyfills.ts",
            "tsConfig": "projects/administration/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/administration/src/favicon.ico",
              "projects/administration/src/assets"
            ],
            "styles": [
              "projects/administration/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/administration/src/environments/environment.ts",
                  "with": "projects/administration/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "administration:build:production"
            },
            "development": {
              "browserTarget": "administration:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "administration:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/administration/src/test.ts",
            "polyfills": "projects/administration/src/polyfills.ts",
            "tsConfig": "projects/administration/tsconfig.spec.json",
            "karmaConfig": "projects/administration/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/administration/src/favicon.ico",
              "projects/administration/src/assets"
            ],
            "styles": [
              "projects/administration/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
